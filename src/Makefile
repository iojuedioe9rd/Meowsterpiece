# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Meowsterpiece_config = debug
  MyApplication_config = debug
endif
ifeq ($(config),release)
  Meowsterpiece_config = release
  MyApplication_config = release
endif

PROJECTS := Meowsterpiece MyApplication

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Meowsterpiece:
ifneq (,$(Meowsterpiece_config))
	@echo "==== Building Meowsterpiece ($(Meowsterpiece_config)) ===="
	@${MAKE} --no-print-directory -C . -f Meowsterpiece.make config=$(Meowsterpiece_config)
endif

MyApplication: Meowsterpiece
ifneq (,$(MyApplication_config))
	@echo "==== Building MyApplication ($(MyApplication_config)) ===="
	@${MAKE} --no-print-directory -C . -f MyApplication.make config=$(MyApplication_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Meowsterpiece.make clean
	@${MAKE} --no-print-directory -C . -f MyApplication.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Meowsterpiece"
	@echo "   MyApplication"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"